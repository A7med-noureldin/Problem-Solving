//Author: Ahmed Noureldin
//Problem link: https://leetcode.com/problems/divide-array-into-equal-pairs/

//Tags: Hash Table, Bit Manipulation, Counting

class Solution {
public:
    bool divideArray(vector<int>& nums) {
        vector<int> v(501, 0);
        for(auto i : nums){
            v[i]++;
        }
        for(auto i : v){
            if(i % 2 != 0)return false;
        }
        return true;
    }
};
