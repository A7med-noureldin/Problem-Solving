#include <bits/stdc++.h>
 
#define ll long long
#define ld long double
#define st first
#define nd second
#define pb push_back
#define fin(a, n) for(int i = a; i < n; i++)
#define fjn(a, n) for(int j = a; j < n; j++)
#define all(a) a.begin(),a.end()
#define allr(a) a.rbegin(),a.rend()
#define FAST ios_base::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr)
 
 
 
using namespace std;
 
const int N = 1e6+5;
vector <ll> ans(N, 0);
 
void sieve() {
    for(ll i = 1; i < N; i++) {
        for(ll j = i; j < N; j += i)ans[j]++;
    }
}
 
ll fastPow(ll x, ll p) {
    ll ret = 1;
    while(p>0) {
        if(p&1)ret *= x;
        x = x * x;
        p /= 2;
    }
    return ret;
}
 
ll divisors(ll x) {
    ll ret = 0;
    for (ll i = 1; i*i <= x; i++) {
        if(x % i == 0) {
            ret++;
            if(i*i != x)ret++;
        }
    }
    return ret;
}
 
void solve()
{
    ll n; cin >> n;
    cout << n << ' ';
    while(n != 1) {
        if(n&1) {
            n *= 3; n++;
        }
        else n /= 2;
        cout << n << ' ';
    }
}
 
int main() {
    FAST;
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif
    ll tt = 1; //cin >> tt;
    while (tt--) {
        solve();
    }
    return 0;
}
